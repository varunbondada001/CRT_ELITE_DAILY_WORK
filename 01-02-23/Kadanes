import java.util.* ;

import java.io.*;

public class Solution {

public static long maxSubarraySum(int[] arr, int n) {

// write your code here

long sum1 = 0;

long maxSum =0;

for(int i=0;i<arr.length;i++){

sum1 += arr[i];

if(sum1>maxSum){

maxSum = sum1;

}

if(sum1<0){

sum1 = 0;

}

}

return maxSum;

}

}
